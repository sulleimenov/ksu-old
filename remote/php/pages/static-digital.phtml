			font: 700 1em/1 "Oswald", sans-serif;
			text-shadow: 0 0 80px rgba(255, 255, 255, .5);
			-webkit-text-fill-color: transparent;
			-webkit-transform: translate3d(0, 0, 0);
			-webkit-backface-visibility: hidden;
		}

		/*
		3f83c0 - 5f93cd
		f8b938 - ffcd54
		81ba5c - 95cc71
		833791 - 9251a1
		e67179 - e88c92
		*/

		.card-links .card.bg-automated {
			background-color: #3f83c0 !important;
		}

		.card-links .card.bg-automated:hover {
			background-color: #5f93cd !important;
		}

		.card-links .card.bg-information {
			background-color: #f8b938 !important;
		}

		.card-links .card.bg-information:hover {
			background-color: #ffcd54 !important;
		}

		.card-links .card.bg-press {
			background-color: #e67179 !important;
		}

		.card-links .card.bg-press:hover {
			background-color: #e88c92 !important;
		}

		.card-links .card.bg-projects {
			background-color: #7602ca !important;
		}

		.card-links .card.bg-projects:hover {
			background-color: #9402f8 !important;
		}

		.card-links .card.bg-feedback {
			background-color: #81ba5c !important;
		}

		.card-links .card.bg-feedback:hover {
			background-color: #95cc71 !important;
		}

		#design #title span {
			background: url(/digital/img/header_bg.png) repeat-y;
			background-clip: text;
			-webkit-background-clip: text;
		}

		#design #title span {
			animation: aitf1 80s linear infinite;
			-webkit-animation: aitf1 80s linear infinite;
		}

		#content .card {
			border: none;
		}

		#content .card-header {
			border-bottom-color: #ffffff;
			border-bottom-width: 4px;
		}

		@-webkit-keyframes aitf1 {
			0% {
				background-position: 0 50%;
			}

			100% {
				background-position: 100% 50%;
			}
		}

		@media (max-width: 767px) {
			#content {
